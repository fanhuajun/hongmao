                        $ydm-imgboxheight:1.1rem;
                        .ydm-imgbox{
                            width:$ydm-imgboxwidth;
                            height:$ydm-imgboxheight;
                            position:absolute;
                            top:$ele-padding;
                            left:$ele-padding;

                            img{
                                width:100%;
                                height:100%;
                            }
                        }
                        .ydm-imgcont{
                            width:100%;
                            height:$ydm-imgboxheight;
                            padding-left:$ydm-imgboxwidth + $ele-padding;
                            position:relative;

                            .c-name{
                                font-size:$ele-size14;
                            }
                            .c-price{
                                padding:.12rem 0;
                                color:$ele-secondcolor;
                                font-size:$ele-size13;
                            }
                            .c-oth{
                                width:100%;
                                position:relative;

                                i{
                                    font-size:$ele-size14;
                                }
                            }
                        }
                        .m-cpgl-cbox{
                            width:100%;
                            padding:$ele-padding;
                            position:relative;

                            &:after{
                                @include lineafter-bottom();
                            }

                        }
                        .m-cpgl-cz{
                            width:100%;
                            padding:$ele-padding;
                            position:relative;
                            
                            &:after{
                                @include clearfix();
                            }
                            a{
                                font-size:$ele-size13;
                                line-height:.5rem;
                                padding:0 .2rem;
                                display:inline-block;
                                color:$ele-fontcolor;
                                border:1px solid $ele-fontcolor;
                                float:left;
                                margin-right:.15rem;

                                i{
                                    font-size:$ele-size16;
                                    margin-right:.05rem;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
