@import "variables","functions","compass/reset";

/*
*global start
*/
*{
    box-sizing:border-box;
}
body{
    font-size: $base-size !important;
    color: $base-color;
    background-color:$base-back;
    font-family:$mainline-ff;
}
h1,h2,h3,h4,h5,h6{
	color:#333;
}
a{
    color:$base-color;
    text-decoration:none;
    @include highlight();
}
.content{
	width:100%;
}

/*--------------------body padding start-----------------------*/
.Tnav{
    padding-top:$Tnav-height;
}
.TBnav{
    padding-top:$Tnav-height;
    padding-bottom:$Bnav-height;
}
.Bnav{
    padding-bottom:$Bnav-height;
}

/*--------------------body padding end-----------------------*/

.default-head{
    width:100%;
    height:$Tnav-height;
    background-color:$ele-backcolor;
    position:fixed;
    top:0;
    left:0;
    text-align:center;
    color:#fff;
    z-index:$z-index2;

    &:after{
        @include clearfix();
    }
    .head-left{
        width:$Tnav-height;
        height:$Tnav-height;
        line-height:$Tnav-height;
        display:block;
        position:absolute;
        top:0;
        left:0;
        z-index:2;
        color:#fff;
        font-size:$ele-size14;

        i{
            font-size:$ele-size19;
            color:#fff;
        }
    }
    .head-tit{
        width:100%;
        line-height:$Tnav-height;
        font-size:$ele-size17;
        padding:0 $Tnav-height;

        @include overflow-1row();
    }
    .head-right{
        width:$Tnav-height;
        height:$Tnav-height;
        line-height:$Tnav-height;
        display:block;
        position:absolute;
        font-size:$ele-size14;
        color:#fff;
        top:0;
        right:0;
        z-index:2;

        i{
            font-size:$ele-size19;
            color:#fff;
            position:relative;

            span{
                padding:0 .1rem;
                font-size:$base-size;
                line-height:.28rem;
                display:inline-block;
                position:absolute;
                top:0;
                right:-3px;
                background-color:red;
                color:#fff;
                @include radius(.15rem);
            }
        }
    }
}

.grade{
    display:inline-block;

    i{
        display: inline-block;
        vertical-align: middle;
        width: .3rem;
        height: .3rem;
        margin-right: 3px;
        background: url(../../images/grade2.png) no-repeat;
        background-size:100% 100%;

        &.grd1{
            background: url(../../images/grade1.png) no-repeat;
            background-size:100% 100%;
        }
        &.grd2{
            background: url(../../images/grade3.png) no-repeat;
            background-size:100% 100%;
        }
    }
}

.default-btn{
    width:100%;
    padding:.55rem $ele-padding;

    a{
        width:100%;
        height:.8rem;
        line-height:.8rem;
        display:block;
        text-align:center;
        font-size:$ele-size14;
        color:#fff;
        background-color:$ele-backcolor;

        @include highlight();
        @include radius(.05rem);
    }
}
.block-box{
    width:100%;
    margin-top:$ele-margin;
    background-color:#fff;
    position:relative;
    
    &:before{
        @include lineafter-top();
    }
    &:after{
        @include lineafter-bottom();
    }
}
.pop{
    position:fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.4);
    z-index:999;
    visibility: hidden;
    opacity: 0;
    -webkit-transition-duration: .3s;
    -moz-transition-duration: .3s;
    -o-transition-duration: .3s;
    -ms-transition-duration: .3s;
    transition-duration: .3s;
    @include highlight();
}
.pop-visible{
    visibility:visible;
    opacity:1;
    cursor:pointer;
}
/*
*global end
*/


/*
*m_index start
*/
.midx-banner{
    width:100%;
    height:4.2rem;
    position:relative;

    img{
        display:inline-block;
        width:100%;
        height:100%;
    }
    .midx-b-pj{
        width:100%;
        height:.64rem;
        line-height:.64rem;
        padding:0 $ele-padding;
        position:absolute;
        bottom:0;
        left:0;
        background:rgba(0,0,0,.6);
        color:#fff;
        font-size:$ele-size13;
    }
}
.midx-b2{
    width:100%;
    padding:.3rem 0;

    li{
        width:1%;
        height:.9rem;
        font-size:$ele-size13;
        display:table-cell;
        vertical-align:middle;
        text-align:center;
        position:relative;
        
        &:after{
            @include lineafter-right();
        }
        &:last-child:after{
            width:0;
        }
        span{
            display:block;
            color:$ele-secondcolor;
        }
        p{
            margin-top:.2rem;
        }
    }
}
.midx-b3{
    width:100%;
    background-color:#fff;

    li{
        width:100%;
        line-height:.55rem;
        padding:$ele-padding;
        position:relative;
        font-size:$ele-size13;

        &:before{
            @include lineafter-bottom();
        }
        &:after{
            @include clearfix();
        }
        span{
            color:#999;
            margin-right:.4rem;
        }
        a{
            display:inline-block;
            float:right;
            width:1.4rem;
            text-align:center;
            color:$ele-fontcolor;
            border:1px solid $ele-fontcolor;

            &.active{
                border-color:#ccc;
                color:#ccc;
            }
        }
    }
    .midx-b3-more{
        width:100%;
        display:block;
        line-height:.74rem;
        text-align:center;
        font-size:$ele-size13;

        i{
            position:relative;
            top:2px;
            font-size:$ele-size16;
        }
    }
}
.midx-palace9{
    width:100%;
    position:relative;

    &:after{
        @include clearfix();
    }
    li{
        width:33.33%;
        height:1.75rem;
        font-size:$ele-size13;
        text-align:center;
        position:relative;
        float:left;
        
        &:before{
            content:'';
            position:absolute;
            left:auto;
            bottom:0;
            right:0;
            top:auto;
            height:100%;
            width:1px;
            background-color:#ddd;
            display:block;
            z-index:2;
            -webkit-transform-origin:100% 50%;
            transform-origin:100% 50%;
            -webkit-transform: scaleX(.5);
            transform: scaleX(.5);
            backface-visibility:hidden;
        }
        &:after{
            content:'';
            position:absolute;
            left:0;
            bottom:0;
            right:auto;
            top:auto;
            height:1px;
            width:100%;
            background-color:#ddd;
            display:block;
            z-index:2;
            -webkit-transform-origin:50% 100%;
            transform-origin:50% 100%;
            -webkit-transform: scaleY(.5);
            transform: scaleY(.5);
            backface-visibility:hidden;
        }
        a{
            display:block;
            padding:.28rem 0;
        }
        .icon-box{
            width:.8rem;
            height:.8rem;
            line-height:.8rem;
            margin:0 auto;
            @include radius50();
        }
        &:nth-child(1) .icon-box{
            background-color:#59d7e5;
        }
        &:nth-child(2) .icon-box{
            background-color:#b3d658;
        }
        &:nth-child(3) .icon-box{
            background-color:#ce8aed;            
        }
        &:nth-child(4) .icon-box{
            background-color:#fa911b;            
        }
        &:nth-child(5) .icon-box{
            background-color:#ff4d7d;            
        }
        &:nth-child(6) .icon-box{
            background-color:#8086f6;            
        }
        &:nth-child(7) .icon-box{
            background-color:#71abe6;            
        }
        &:nth-child(8) .icon-box{
            background-color:#fe4545;            
        }
        &:nth-child(9) .icon-box{
            background-color:#f9911a;            
        }
        i{
            font-size:$ele-size18;
            color:#fff;
        }
        label{
            display:inline-block;
            margin-top:.15rem;
        }
    }
}
/*
*m_index end
*/

