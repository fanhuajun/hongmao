/*
*m_caipuguanli start
*/
$ydm-searchbox:1.06rem;
.TSnav{
    padding-top:$Tnav-height + $ydm-searchbox;
}
.ydm-searchbox{
    width:100%;
    height:$ydm-searchbox;
    position:fixed;
    top:$Tnav-height;
    left:0;
    z-index:$z-index1;
    background-color:$base-back;
    
    &:after{
        content:'';
        position:absolute;
        left:0;
        bottom:0;
        right:auto;
        top:auto;
        height:1px;
        width:100%;
        background-color:#bbb;
        display:block;
        z-index:2;
        -webkit-transform-origin:50% 100%;
        transform-origin:50% 100%;
        -webkit-transform: scaleY(.5);
        transform: scaleY(.5);
        backface-visibility:hidden;
    }
    .m-cpgl-nav{
        width:100%;
        position:relative;
        background-color:#fff;

        &:after{
            @include lineafter-bottom();
        }
        li{
            width:1%;
            height:.84rem;
            display:table-cell;
            vertical-align:middle;
            text-align:center;
            font-size:$ele-size13;
            position:relative;
            
            &:after{
                content:"";
                position:absolute;
                top:.1rem;
                left:0;
                width:1px;
                height:.64rem;
                background-image:linear-gradient(to bottom,#fff,#fff,#888,#fff,#fff);
            }
            &:first-child:after{
                display:none;
            }
            a{
                display:block;
            }
        }
    }
}

$yscroll-Lwidth:1.7rem;
.ydm-area{
    width:$yscroll-Lwidth;
    position:fixed;
    top:$Tnav-height + $ydm-searchbox;
    left:0;
    bottom:0;
    right:0;
    z-index:10;
    
    &:after{
        content:'';
        position:absolute;
        left:auto;
        bottom:0;
        right:0;
        top:auto;
        height:100%;
        width:1px;
        background-color:#bbb;
        display:block;
        z-index:2;
        -webkit-transform-origin:100% 50%;
        transform-origin:100% 50%;
        -webkit-transform: scaleX(.5);
        transform: scaleX(.5);
        backface-visibility:hidden;
    }
    .yscroll-left{
        width:100%;
        height:100%;
        position:absolute;
        top:0;
        left:0;
        overflow:auto;
        z-index:2;
        -webkit-overflow-scrolling:touch;
        -moz-overflow-scrolling:touch;
        -ms-overflow-scrolling:touch;
        overflow-scrolling:touch;
        background-color:#f3f4f6;   

        .left-category{
            width:100%;
            -webkit-overflow-scrolling:touch;
            -moz-overflow-scrolling:touch;
            -ms-overflow-scrolling:touch;
            overflow-scrolling:touch;

            li{
                width:100%;
                height:.9rem !important;
                line-height:.9rem;
                font-size:$ele-size14;
                color:#333;
                position:relative;

                a{
                    padding-left:.3rem;
                    display:block;
                    -webkit-tap-highlight-color:rgba(0,0,0,0);
                }
                &:after{
                    content:'';
                    position:absolute;
                    left:0;
                    bottom:0;
                    right:auto;
                    top:auto;
                    height:1px;
                    width:100%;
                    background-color:#ddd;
                    display:block;
                    z-index:2;
                    -webkit-transform-origin:50% 100%;
                    transform-origin:50% 100%;
                    -webkit-transform: scaleY(.5);
                    transform: scaleY(.5);
                    backface-visibility:hidden;
                }
                &.swiper-pagination-bullet-active{
                    background-color:#fff;
                    color:$ele-fontcolor;
                    
                    a{
                        color:$ele-fontcolor;
                    }
                    &:before{
                        content:'';
                        position:absolute;
                        left:-1px;
                        bottom:0;
                        right:auto;
                        top:auto;
                        height:100%;
                        width:3px;
                        background-color:$ele-fontcolor;
                        display:block;
                        z-index:2;
                        -webkit-transform-origin:100% 50%;
                        transform-origin:100% 50%;
                        -webkit-transform: scaleX(.5);
                        transform: scaleX(.5);
                        backface-visibility:hidden;
                    }
                }
            }
        }
    }
}
.ydm-container{
    width:100%;
    height:100%;
    padding-left:$yscroll-Lwidth;
    position:relative;
    overflow:hidden;

    .ydm-swiper{
        width:100%;
        height:100%;

        position:relative;

        .ydm-wrapper{
            width:100%;
            height:100%;
            

            .ydm-slide{
