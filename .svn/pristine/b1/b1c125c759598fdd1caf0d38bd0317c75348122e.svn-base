.xgcp-pop{
    position:fixed;
    top:1.9rem;
    left:.2rem;
    right:.2rem;
    font-size:$ele-size13;
    background-color:#fff;
    z-index:1000;
    visibility:hidden;
    opacity:0;
    @include duration(.3s);

    &.show{
        opacity:1;
        visibility:visible;
    }
    .pop-tit{
        width:100%;
        font-size:$ele-size16;
        line-height:.7rem;
        text-align:center;
        position:relative;
        
        &:after{
            @include lineafter-bottom();
        }
        a{
            position:absolute;
            display:block;
            top:0;
            right:0;
            width:.7rem;
            text-align:center;

            i{
                font-size:.5rem;
                color:#999;
                position:relative;
                top:-1px;
            }
        }
    }
    .pop-cont{
        width:100%;
        position:relative;
        height:5rem;

        &:after{
            @include lineafter-bottom();
        }
        $xgcp-popleft:1.6rem;
        .pop-left{
            width:$xgcp-popleft;
            height:5rem;
            position:absolute;
            top:0;
            left:0;
            z-index:10;
            overflow:auto;
            border-right:1px solid #eee;
            background-color:$base-back;
            @include touch();
            
            ul{
                width:100%;

                li{
                    width:100%;
                    line-height:.86rem;
                    position:relative;
                    
                    &:after{
                        @include lineafter-bottom();
                    }
                    a{
                        display:block;
                        padding-left:.3rem;
                        position:relative;

                        &.active{
                            color:$ele-fontcolor;
                            background-color:#fff;

                            &:before{
                                content:'';
                                position:absolute;
                                left:0;
                                bottom:0;
                                right:auto;
                                top:auto;
                                height:100%;
                                width:3px;
                                background-color:$ele-fontcolor;
                                display:block;
                                z-index:2;
                                -webkit-transform-origin:100% 50%;
                                transform-origin:100% 50%;
                                -webkit-transform: scaleX(.5);
                                transform: scaleX(.5);
                                backface-visibility:hidden;
                            }
                        }
                    }
                }
            }
        }
        .pop-right{
            height:5rem;
            position:absolute;
            top:0;
            left:$xgcp-popleft;
            right:0;
            z-index:9;
            overflow:auto;
            @include touch();
        }
    }
    .pop-c{
        width:100%;
        line-height:.36rem;
        padding:$ele-padding $ele-padding 0 $ele-padding;

        .xzlr{
            width:100%;
        }
    }
    .pop-btnbox{
        width:100%;
        padding:.3rem $ele-padding;
        position:relative;
        
        &:after{
            @include clearfix();
        }
        a{
            width:2.4rem;
            line-height:.7rem;
            font-size:$ele-size13;
            display:block;
            background-color:$ele-backcolor;
            color:#fff;
            float:right;
            text-align:center;
        }
    }
}
